class Solution {
    public int findContentChildren(int[] g, int[] s) {
        Arrays.sort(g);
        Arrays.sort(s);
        int c = 0; 
        int l = s.length - 1; 
        for (int i = g.length - 1; i >= 0; i--) {
            if (l >= 0 && g[i] <= s[l]) {
                c++; 
                l--;
            }
        }
        return c;
    }
}
//Leetcode  Assign Cookies
